<%= form_for :kitten do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :softness %>
    <%= f.select :softness, options_for_select((1..10)) %>
    <%= f.label :cuteness %>
    <%= f.select :cuteness, options_for_select((1..10)) %>
    <%= f.label :age %>
    <%= f.number_field :age %>
<% end %>