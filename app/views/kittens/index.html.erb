<h1>All kittens</h1>

<% @kittens.each do |kitten| %>
    <h2><%= kitten.name %></h2>
    <b><p>Age:</p></b>
    <p><%= kitten.age %></p>
    <b><p>Softness:</p></b>
    <p><%= kitten.softness %></p>
    <b><p>Cuteness:</p></b>
    <p><%= kitten.cuteness %></p>
    <%= button_to "Delete", kitten, :method => :delete %>
    <%= link_to "Edit", edit_kitten_url(kitten) %>
<% end %>